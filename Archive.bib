@MASTERSTHESIS{Yip2015,
  author = {Yip, Ka Wa},
  title = {Optical pumping of multiple atoms in the single photon subspace of
	two-mode cavity QED},
  school = {Miami University},
  year = {2015},
  abstract = {We consider N four level atoms coupled to an orthogonally polarized,
	degenerate two-mode optical cavity. Starting with the atoms prepared
	in one of the degenerate ground states a single photon introduced
	into the driven cavity mode will be recycled to pump multiple atoms
	to the other ground state. For two atoms we analytically calculate
	the steady state using quantum trajectory equations and show that
	the system makes stochastic transitions between two different subspaces
	with the transition correlated with the emission of a polarized photon
	from one of the two modes of the cavity. In this way the long time
	evolution of the atomic state can be monitored by direct photodetection
